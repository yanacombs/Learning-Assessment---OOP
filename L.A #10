class Vehicle:
    def __init__ (self, brand, model, fuel_type):
        self.brand = brand
        self.model = model
        self.fuel_type = fuel_type
    def describeVehicle(self):
        print(f"My car brand is {self.brand}, the model of it is {self.model} and it is {self.fuel_type}")
           
class Car(Vehicle):
    pass
Audi = Car("Audi", "Audi A8", "Electric")
Audi.describeVehicle()
class Motorcycle(Vehicle):
    pass
BMW_Motorrad = Motorcycle("BMW Motorrad", "S 1000 XR", "Diesel")
BMW_Motorrad.describeVehicle()
