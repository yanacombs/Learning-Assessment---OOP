class Party:
    def __init__(self, theme, food, special, gifts):
        self.theme = theme
        self.food = food
        self.special = special
        self.gifts = gifts
   
    def dea(self):
        print(f'''Theme: {self.theme}
Food: {self.food}
Special Food: {self.special}''')
        self.__foods()

    def __foods(self):
        print(f"The Secret dish of {self.theme} is {self.gifts}")

partytheme = Party("Christmas Party", " Mango Graham", "Cake", "Brownies")
partytheme2 = Party("BDay", "Spaghetti", "kare-kare", "Pancit")
partytheme.dea()
print("===========================================================")
partytheme2.dea()
